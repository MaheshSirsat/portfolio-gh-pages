<c:FragmentDefinition
    xmlns="sap.uxap"
    xmlns:c="sap.ui.core"
    xmlns:m="sap.m"
    xmlns:l="sap.ui.layout.form"
>
    <ObjectPageSection
        titleUppercase="false"
        title="Education"
        importance="Medium"
    >
        <subSections>
            <ObjectPageSubSection
             mode="Expanded"
                title="Savitribai Phule Pune University"
                titleUppercase="false"
            >
                <blocks>
                    <m:VBox class="sapUiMediumMargin">
                        <l:SimpleForm
                            layout="ResponsiveGridLayout"
                            editable="false"
                        >
                            <m:Label wrapping="true" text="Speciliazation" />
                            <m:Text wrapping="true" text="Computer Engineering" />
                            <m:Label wrapping="true" text="Degree" />
                            <m:Text wrapping="true" text="B.tech" />
                            <m:Label wrapping="true" text="CGPA" />
                            <m:Text wrapping="true" text="8.92" />
                            <m:Label wrapping="true" text="Duration" />
                            <m:Text wrapping="true" text="June 2019-June 2023" />
                        </l:SimpleForm>
                    </m:VBox>
                </blocks>
            </ObjectPageSubSection>
            <ObjectPageSubSection>
                <blocks>
                    <m:VBox />
                </blocks>
            </ObjectPageSubSection>
        </subSections>
    </ObjectPageSection>
</c:FragmentDefinition>
